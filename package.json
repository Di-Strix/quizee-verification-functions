{
  "name": "@di-strix/quizee-verification-functions",
  "version": "2.1.5",
  "description": "Set of verification functions to validate quizee",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": "github:Di-Strix/quizee-verification-functions",
  "files": [
    "dist/**/*"
  ],
  "exports": {
