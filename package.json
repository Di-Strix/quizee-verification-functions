      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig-cjs.json tsconfig-esm.json tsconfig-types.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "quizee"
  ],
  "author": "Andrew Ushakov",
  "license": "MIT",
  "dependencies": {
    "joi": "^17.6.0"
  },
  "devDependencies": {
    "@di-strix/quizee-types": "^3.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "^18.14.2",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
